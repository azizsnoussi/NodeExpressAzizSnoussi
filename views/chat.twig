<!DOCTYPE html>
<html>
    <head>
        <script src="/socket.io/socket.io.js"></script>
    </head>
    <body>
        {# <p>hello</p> #}

        <script>
            const socket = io.connect("ws://localhost:3000");
            socket.on('msg',(data)=>{
                console.log('data :',data);
                let p=document.createElement('p');
                p.innerText=data;
                document.body.append(p);
            });
        </script>
    </body>
</html>